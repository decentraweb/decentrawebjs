{
  "name": "@decentraweb/core",
  "version": "2.0.0-alpha.2",
  "description": "Decentraweb javascript library",
  "main": "build/esm/index.js",
  "types": "build/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./build/esm/index.d.ts",
        "default": "./build/esm/index.js"
      },
      "require": {
        "types": "./build/cjs/index.d.ts",
        "default": "./build/cjs/index.js"
      }
    },
    "./api": {
      "import": {
        "types": "./build/esm/api/index.d.ts",
        "default": "./build/esm/api/index.js"
      },
      "require": {
        "types": "./build/cjs/api/index.d.ts",
        "default": "./build/cjs/api/index.js"
      }
    },
    "./contracts": {
      "import": {
        "types": "./build/esm/contracts/index.d.ts",
        "default": "./build/esm/contracts/index.js"
      },
      "require": {
        "types": "./build/cjs/contracts/index.d.ts",
        "default": "./build/cjs/contracts/index.js"
      }
    },
    "./registrars": {
      "import": {
        "types": "./build/esm/registrars/index.d.ts",
        "default": "./build/esm/registrars/index.js"
      },
      "require": {
        "types": "./build/cjs/registrars/index.d.ts",
        "default": "./build/cjs/registrars/index.js"
      }
    }
  },
  "scripts": {
    "prepare": "npm run build",
    "build:esm": "tsc --project tsconfig.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs",
    "build:browser": "webpack --progress --config webpack.config.cjs",
    "test:e2e": "mocha test/e2e/**/*.test.ts",
    "watch": "tsc --watch --project tsconfig.json",
    "watch:cjs": "tsc --watch --project tsconfig.cjs.json",
    "watch:browser": "webpack --watch --progress --config webpack.config.cjs --mode development"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ensdomains/address-encoder": "^0.2.22",
    "@ensdomains/content-hash": "^2.5.7",
    "bs58": "^5.0.0",
    "dns-packet": "^5.6.1",
    "fetch-ponyfill": "^7.1.0",
    "idna-uts46-hx": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.19",
    "@babel/plugin-proposal-decorators": "^7.22.15",
    "@babel/plugin-syntax-import-attributes": "^7.22.5",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@types/chai": "^4.3.6",
    "@types/chance": "^1.1.4",
    "@types/mocha": "^10.0.1",
    "babel-loader": "^9.1.2",
    "buffer": "^6.0.3",
    "chai": "^4.3.8",
    "chance": "^1.1.11",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.32.2",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "ethers": "^5.7.0"
  }
}
