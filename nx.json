{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "build:browser"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/build"
      ]
    },
    "build:browser": {
      "dependsOn": [
        "^build:browser"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    }
  }
}
