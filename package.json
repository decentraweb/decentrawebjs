{
  "name": "decentrawebjs",
  "repository": "https://github.com/decentraweb/decentrawebjs",
  "version": "1.0.0",
  "description": "",
  "main": "packages/resolver/src/index.js",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "gateway": "npm run start --workspace=@decentraweb/http-gateway",
    "resolver": "npm run start --workspace=@decentraweb/resolver",
    "manage": "npm run start --workspace=@decentraweb/cli",
    "wiki:init": "git remote add wiki git@github.com:decentraweb/decentrawebjs.wiki.git",
    "wiki:pull": "git subtree pull --prefix docs wiki master --squash --message=\"Merge wiki updates into docs.\"",
    "wiki:push": "git subtree push --prefix docs wiki master"
  },
  "workspaces": [
    "./packages/*"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.18.31",
    "lerna": "^6.6.2",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
